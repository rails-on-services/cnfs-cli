# frozen_string_literal: true

module CnfsCli
  module Plugins
    class <%= name.camelize %>
      class << self
        def initialize_<%= name %>
          require 'cnfs_cli/<%= name %>'
          Cnfs.logger.info "[<%= name.camelize %>] Initializing from #{gem_root}"
        end

        def gem_root
          CnfsCli::<%= name.classify %>.gem_root
        end
      end
    end
  end
end
