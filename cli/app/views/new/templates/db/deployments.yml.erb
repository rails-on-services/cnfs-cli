# <%= name %> deployments.yml
---
DEFAULTS: &DEFAULTS
  name: $LABEL

<%= "#{name}_development: &#{name}_development" %>
  <<: *DEFAULTS
  application: <%= name %>
  namespace: development
  key: development

<%= "#{name}_test: &#{name}_test" %>
  <<: *DEFAULTS
  application: <%= name %>
  namespace: test
  key: test

<%= "#{name}_production: &#{name}_production" %>
  <<: *DEFAULTS
  application: <%= name %>
  target: production
  key: production
