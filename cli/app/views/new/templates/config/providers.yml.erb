# <%= name %> providers.yml
---
localstack:
  name: $LABEL
  type: Provider::Aws
  config:
    account_id: 123456789012
    access_key_id: access_key_id
    secret_access_key: secret_access_key
    region: us-east-1
    storage:
      endpoint: http://localstack:4572
      force_path_style: yes
      region: localstack
    mq:
      endpoint: http://localstack:4576
      region: localstack
