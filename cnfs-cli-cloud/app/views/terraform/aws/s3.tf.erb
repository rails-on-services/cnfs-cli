# S3 buckets
<%-
@module_name = :s3
def defaults
  {
    source: 'rails-on-services/s3/aws'
  }
end
-%>
<%- resource.resources.each do |key, config| -%>

module "<%= title key %>" {
<%= resource_config(target, resource, defaults, config).render.join("\n") %>
}

output "<%= title key %>" {
  value = module.s3.this
}
<% end -%>
