# AWS SES
<%-
@module_name = :ses
def defaults
  {
    source: 'rails-on-services/ses/aws',
    domain_name: target.domain_name,
    aws_profile: target.provider.profile
  }
end
-%>

module "<%= title %>" {
<%= render_config(defaults).join("\n") %>

  route53_zone_id = module.route53.this.zone_id
}
